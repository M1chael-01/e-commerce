#JWT Configuration
JWT_SECRET=your_jwt_secret_key_here  # Use a strong random string

#Stripe Payment Configuration
STRIPE_PRIVATE_KEY=your_stripe_secret_key_here

#Session Key for Express Sessions
SESSION_KEY=your_session_secret_here

#PostgreSQL Database Configuration
DB_USER=admin_ecommerce               # Custom DB username (if used)
DB_PASSWORD=your_db_password_here     # DB password

PGUSER=your_postgres_username         # Default: postgres
PGHOST=localhost                      # Usually localhost in development
PGDATABASE=your_database_name         # Name of your DB (e.g., ecommerce_app)
PGPASSWORD=your_postgres_password     # Your PostgreSQL password
PGPORT=5432                           # Default PostgreSQL port
